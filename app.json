{
  "expo": {
    "name": "restaurant-chooser",
    "slug": "restaurant-chooser",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "restaurant-chooser",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fun.xread.restaurantchooser"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "fun.xread.restaurantchooser"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "enableFullScreenImage_legacy": true,
          "resizeMode": "cover",
          "ios": {
            "image": "./assets/images/img/splash.png",
            "backgroundColor": "#ffffff"
          },
          "android": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/img/splash.png",
            "imageWidth": 500
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
